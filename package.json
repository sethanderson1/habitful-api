{
  "name": "habitful-api",
  "version": "1.0.0",
  "description": "Track progress on your habits with Habitful!",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require test/setup.js",
    "dev": "nodemon src/server.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "cross-env NODE_ENV=test npm run migrate",
    "migrate:production": "cross-env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
    "start": "node src/server.js",
    "predeploy": "npm audit",
    "deploy": "git push heroku master"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sethanderson1/express-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/sethanderson1/express-boilerplate/issues"
  },
  "homepage": "https://github.com/sethanderson1/express-boilerplate#readme"
}
